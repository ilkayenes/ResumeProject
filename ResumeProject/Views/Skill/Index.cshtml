@model  List<ResumeProject.Models.Tbl_Skill>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2 >Yetenekler</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>SkillTitle</th>
        <th>SkillDescription</th>
        <th>Skillicon</th>
        <th>Delete Skill</th>
        <th>Update Skill</th>
    </tr>


    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.SkillID</td>
                    <td>@item.SkillTitle</td>
                    <td>@item.SkillDescription</td>
                    <td>@item.Skillicon</td>
                    <td><a href="/Skill/DeleteSkill/@item.SkillID" class="btn btn-outline-danger">Delete</a></td>
                    <td><a href="/Skill/UpdateSkill/@item.SkillID" class="btn btn-outline-success">Update</a></td>
                </tr>
            }
</table>
<a href="/Skill/AddSkill/" class="btn btn-outline-primary">Add New Skill</a>