
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>İstatistik Sayfası</h2>
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>

    <div class="container">
        <h2></h2>
        <div class="card">
            <div class="card-body">Proje Taleb adı altında gelen mesaj sayısıSayısı: @ViewBag.ProjeTalebsayi</div>
        </div>
        <br>
        <div class="card bg-primary text-white">
            <div class="card-body">Bildiğim Programlama Dili Sayısı:@ViewBag.TechnologyCount </div>
        </div>
        <br>
        <div class="card bg-success text-white">
            <div class="card-body">C# Puanı: @ViewBag.CsharpValue </div>
        </div>
        <br>
        <div class="card bg-info text-white">
            <div class="card-body">Toplam Mesaj: @ViewBag.ContactCount</div>
        </div>
        <br>
        <div class="card bg-warning text-white">
            <div class="card-body">Teşekkür Mesaj Sayısı @ViewBag.subjectTesekkür</div>
        </div>
        <br>
        <div class="card bg-danger text-white">
            <div class="card-body">Teknoloji Değerleri Toplamı @ViewBag.sumtechnologyvalue</div>
        </div>
        <br>
        <div class="card bg-secondary text-white">
            <div class="card-body">Teknoloji Değerleri Ortalaması: @ViewBag.averageTechnologyvalue </div>
        </div>
        <br>
        <div class="card bg-dark text-white">
            <div class="card-body">1 numaralı ID: @ViewBag.Getby1ID </div>
        </div>
        <br>
        <div class="card bg-light text-dark">
            <div class="card-body">En Yüksek Teknoloji Değeri: @ViewBag.MaxTechnologyValue </div>
        </div>
    </div>

</body>
</html>



