
@{
    ViewBag.Title = "AddProfile";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>AddProfile</h2>


<form method="post">

    <label> Profile Title</label>
    <input type="text" class="form-control" name="ProfileTitle" />
    <br />

    <label>Profile Description</label>
    <input type="text" class="form-control" name="ProfileDescription" />
    <br />

    <label>Name </label>
    <input type="text" class="form-control" name="Name" />
    <br />

    <label>Mail</label>
    <input type="text" class="form-control" name="Mail" />
    <br />

    <label>Address</label>
    <input type="text" class="form-control" name="Address" />
    <br />

    <label>Phone</label>
    <input type="text" class="form-control" name="Phone" />
    <br />

    <label>SocialMedia1</label>
    <input type="text" class="form-control" name="SocialMedia1" />
    <br />

    <label>SocialMedia2</label>
    <input type="text" class="form-control" name="SocialMedia2" />
    <br />

    <label>SocialMedia3</label>
    <input type="text" class="form-control" name="SocialMedia3" />
    <br />

    <button class="btn btn-primary">Save</button>
</form>
